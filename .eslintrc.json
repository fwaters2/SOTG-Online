{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/prop-types": 0,
    "import/no-unresolved": "off",
    
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "allow-ternary:":true
      }
    ]
  }
}
